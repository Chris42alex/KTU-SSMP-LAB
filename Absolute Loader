#include <stdio.h>
#include <string.h>
#include <stdlib.h>

int main() {
    FILE *fp1;
    int i, j, hexaddr;
    char line[50], addr[10];

    fp1 = fopen("input.txt", "r");

    do {
        fscanf(fp1, "%s", line);

        if(line[0] == 'T') {
            for(i = 1, j = 0; j <= 6; i++, j++)
                addr[j] = line[i];
            addr[j] = '\0';

            hexaddr = (int)strtol(addr, NULL, 16);  // Convert to integer
        }

        i = 9;
        while(line[i] != '\0') {
            printf("%X\t%c%c\n", hexaddr, line[i], line[i + 1]);
            hexaddr++;
            i += 2;
        }

    } while(!feof(fp1));

    fclose(fp1);
    return 0;
}
